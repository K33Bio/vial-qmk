{

	"keyboard_name": "Dactyl 5key 4x5",
	"maintainer": "PartWizard",
	"manufacturer": "K33B",
	"url": "https://k33b.com",
	"bootloader_instructions": "Left half: press top-left key (ESC) while plugging USB cable. Right half: press top-right key (BACKSPACE) while plugging USB cable.",
	"tags": ["split", "column stagger", "ergonomic", "curved", "RP2040", "MX"],
	"bootloader": "rp2040",
	"processor": "RP2040",

	"usb": {
		"device_version": "1.0.0",
		"vid": "0x1029",
		"pid": "0xD33E",
		"force_nkro": true
	},

	"features": {
		"magic": true,
		"mousekey": true,
		"extrakey": true,
		"console": false,
		"combo": false,
		"nkro": true,
		"audio": true,
		"key_override": true,
		"leader": true,
		"unicode": true,
		"midi": true
	},

	"caps_word": {
		"enabled": true,
		"both_shifts_turns_on": true,
		"double_tap_shift_turns_on": true,
		"invert_on_shift": true,
		"idle_timeout": 5000
	},

	"bootmagic": {
		"enabled": true,
		"matrix": [0,0]
	},	

	"split": {
		"enabled": true,
		"bootmagic": { "matrix": [5,0] },
		"soft_serial_pin": "GP28",
		"transport": {
			"protocol": "serial", 
			"watchdog": true
		},
		"usb_detect": { "enabled": false }
	},

	"diode_direction": "ROW2COL",
	"matrix_pins": {
		"rows": ["GP0", "GP1", "GP2", "GP3", "GP4"],
		"cols": ["GP5", "GP6", "GP7", "GP8", "GP9"]
	},
	
